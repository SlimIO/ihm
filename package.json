{
    "name": "@slimio/ihm",
    "version": "0.1.0",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "keywords": [
        "SlimIO",
        "interface",
        "ihm"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "description": "Interface HTML/CSS for addons",
    "dependencies": {
        "@polka/send-type": "^0.5.2",
        "@slimio/addon": "^0.18.1",
        "polka": "^0.5.2",
        "sirv": "^0.4.0",
        "zup": "0.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^8.0.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/alert": "^0.2.0",
        "@slimio/eslint-config": "^3.0.2",
        "@slimio/metrics": "^0.4.0",
        "@slimio/psp": "^0.5.3",
        "ava": "^1.4.1",
        "cross-env": "^5.2.0",
        "dotenv": "^8.0.0",
        "eslint": "^6.0.1",
        "husky": "^3.0.0",
        "jsdoc": "^3.6.2",
        "nyc": "^14.0.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js src/**.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
