{
    "name": "@slimio/interface",
    "version": "0.1.0",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "keywords": [],
    "author": "SlimIO",
    "license": "MIT",
    "description": "Interface HTML/CSS for addons",
    "dependencies": {
        "@polka/send-type": "^0.5.2",
        "@slimio/addon": "^0.17.1",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "polka": "^0.5.2",
        "sirv": "^0.4.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/alert": "^0.2.0",
        "@slimio/eslint-config": "^2.0.4",
        "@slimio/metrics": "^0.4.0",
        "ava": "^1.4.1",
        "dotenv": "^7.0.0",
        "eslint": "^5.16.0",
        "husky": "^2.2.0",
        "jsdoc": "^3.5.5",
        "nyc": "^14.0.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
